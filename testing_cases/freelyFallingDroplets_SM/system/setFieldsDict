FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.gas 1
    volScalarFieldValue alpha.liquid 0
);

regions
(
    boxToCell
    {
        box (1.9 -1 1.9) (2.1 1 2.1);
        fieldValues
        (
            volScalarFieldValue alpha.gas 0.9999
            volScalarFieldValue alpha.liquid 0.0001
        );
    }
);



// ************************************************************************* //
